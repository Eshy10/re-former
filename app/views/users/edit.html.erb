<%= form_for @user do |f| %>
    <%= f.label :usernmae %>
    <%= f.text_field :username, placeholder: "username" %><br><br>
    <%= f.label :email %>
    <%= f.text_field :email, placeholder: "email" %><br><br>
    <%= f.label :password %>
    <%= f.text_field :password, placeholder: "password" %><br><br>
    <%= f.submit "Edit" %>
<% end %>